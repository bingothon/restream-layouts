{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"additionalProperties": false,
	"required": [
		"members"
	],
	"properties": {
		"members": {
			"type": "array",
			"items": {
				"required": [
					"id",
					"name",
					"avatar",
					"isSpeaking"
				],
				"type": "object",
				"properties": {
					"id": {
						"type": "string",
						"default": ""
					},
					"name": {
						"type": "string",
						"default": ""
					},
					"avatar": {
						"type": "string",
						"default": ""
					},
					"isSpeaking": {
						"type": "boolean",
						"default": false
					}
				}
			}
		}
	}
}
