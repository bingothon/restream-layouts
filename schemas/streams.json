{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "array",
	"default": [],
	"items": {
			"type": "object",
			"additionalProperties": "false",
			"required": [],
		"properties": {
			"channel": {
				"type": "string",
				"default": "esamarathon"
			},
			"widthPercent": {
				"type": "number",
				"default": "100"
			},
			"heightPercent": {
				"type": "number",
				"default": "100"
			},
			"topPercent": {
				"type": "number",
				"default": "0"
			},
			"leftPercent": {
				"type": "number",
				"default": "0"
			},
			"quality": {
				"type": "string",
				"enum": [ "chunked" ],
				"default": "chunked"
			},
			"volume": {
				"type": "number",
				"default": "0.5"
			},
			"paused": {
				"type": "boolean",
				"default": "false"
			},
			"hidden": {
				"type": "boolean",
				"default": "true"
			}
		}
	}
}
