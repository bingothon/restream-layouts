{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "array",
	"default": [],
	"items": {
			"type": "object",
			"additionalProperties": false,
			"required": ["channel","widthPercent","heightPercent","topPercent","leftPercent","quality","volume","paused","delay","availableQualities"],
		"properties": {
			"channel": {
				"type": "string",
				"default": "esamarathon"
			},
			"widthPercent": {
				"type": "number",
				"default": 100
			},
			"heightPercent": {
				"type": "number",
				"default": 100
			},
			"topPercent": {
				"type": "number",
				"default": 0
			},
			"leftPercent": {
				"type": "number",
				"default": 0
			},
			"quality": {
				"type": "string",
				"default": "chunked"
			},
			"volume": {
				"type": "number",
				"default": 0.5
			},
			"paused": {
				"type": "boolean",
				"default": false
			},
			"delay": {
				"type":"number",
				"default":0
			},
			"availableQualities": {
				"type": "array",
				"default": [],
				"items": {
					"type": "object",
					"additionalProperties": false,
					"required": ["name","group"],
					"properties": {
						"name": {"type":"string"},
						"group": {"type":"string"}
					}
				}
			}
		}
	}
}
